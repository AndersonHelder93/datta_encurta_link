# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
indent_style = space
indent_size = 2 #tamanho da identação
end_of_line = crlf
charset = utf-8
trim_trailing_whitespace = true #tira os espaços em brancos #
insert_final_newline = true #insere uma linha abaixo do código#